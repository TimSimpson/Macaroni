class Something
{
	private int x;
	private int y;

	private Something()
	{
	}

	public static Something Create(int x, int y)
	{
		Something s;
		s.SetX(x);
		s.SetY(y);
		return s;
	}

	public int GetX() const
	{
		return this->x;
	}

	public int GetY() const
	{
		return this->y;
	}

	~hidden void SetX(int value)
	{
		this->x = value;
	}

	~hidden void SetY(int value)
	{
		this->y = value;
	}

};
