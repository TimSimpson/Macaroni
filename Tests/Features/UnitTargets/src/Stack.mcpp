~import std::vector;


~namespace Example::Containers;

class Stack
{
    private vector<int> vec;

    public Stack()
    :   vec()
    {
    }

    public void pop()
    {
        int value =  vec[vec.size() - 1];
        vec.pop_back();
    }

    public void push(int value)
    {
        vec.push_back(value);
    }
};
